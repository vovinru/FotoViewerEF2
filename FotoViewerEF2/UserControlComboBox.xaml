<UserControl x:Class="FotoViewerEF2.UserControlComboBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="100">
    <Grid>
        <ComboBox Name="AutoCompleteComboBox" IsEditable="True" IsTextSearchEnabled="False" SelectedItem="{Binding SelectedItem}" IsReadOnly="False"
                  PreviewTextInput="AutoCompleteComboBox_PreviewTextInput"
                  PreviewKeyUp="AutoCompleteComboBox_PreviewKeyUp"
                  DataObject.Pasting="AutoCompleteComboBox_Pasting"
                  GotKeyboardFocus="AutoCompleteComboBox_GotKeyboardFocus" 
                  LostKeyboardFocus="AutoCompleteComboBox_LostKeyboardFocus"/>
    </Grid>
</UserControl>
